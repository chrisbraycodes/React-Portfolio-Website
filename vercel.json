{
  "rewrites": [
    {
      "source": "/myreads/:path*",
      "destination": "https://my-reads-blush.vercel.app/:path*"
    },
    {
      "source": "/rickandmorty/:path*",
      "destination": "https://rickandmortyapishowcaase.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/myreads/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://chrisbraycodes.com;"
        }
      ]
    },
    {
      "source": "/rickandmorty/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://chrisbraycodes.com;"
        }
      ]
    }
  ]
}

